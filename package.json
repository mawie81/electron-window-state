{
  "name": "electron-window-state",
  "version": "5.0.3",
  "description": "Simple module that helps to save and restore size and position of Electron windows.",
  "license": "MIT",
  "repository": "mawie81/electron-window-state",
  "main": "index.js",
  "author": "Marcel Wiehle",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "keywords": [
    "electron"
  ],
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "jsonfile": "^4.0.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "ava": "^5.1.0",
    "mockery": "^2.1.0",
    "sinon": "^6.1.5",
    "xo": "^0.22.0"
  },
  "xo": {
    "space": true,
    "ignores": [
      "index.d.ts"
    ],
    "rules": {
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "prefer-object-spread": 0
    }
  }
}
